{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [1],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "camelcase": [0],
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "handle-callback-err": [0],
    "semi": [1, "always"],
    "indent": [2, 2],
    "no-undef": [2]
  }
}
